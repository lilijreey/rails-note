.span12
  = form_for @group do |f|
    = f.label :title , 'Title' , class:'input-xxlarge'
    = f.text_field :title

    %br/
    = f.label :description, 'Description'
    = f.text_field :description


    .form-actions
      = f.submit 'Submit', :disable_with => 'Submiting...', :class => 'btn btn-primary'

